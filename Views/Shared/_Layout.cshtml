                        function myCallback() {
                            $('.search-easyAutocomplete, .search-easyAutocomplete.mobile').on('keydown blur', function (e) {
                                if (e.keyCode == 13) {
                                    e.preventDefault();
                                    $('.gsc-search-button').click();
                                }
                                $('#custom-search-input').tooltip('hide');
                                $('.gsc-input').val($(this).val());
